{
  "compilerOptions": {
    "importsNotUsedAsValues": "error",
    "resolveJsonModule": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "composite": true,
    "isolatedModules": false,
    "declaration": true,
    "declarationMap": true,
    "types": [
      "vite/client"
    ]
  },
  "include": [
    "src",
    "vite.config.ts"
  ]
}